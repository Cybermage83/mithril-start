{
  "name": "start",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel security-check open:src lintjs:watch lintsass:watch start-mockapi",
    "open:src": "babel-node build/srcServer.js",
    "lintjs": "esw 'src/**/*.js'",
    "lintjs:watch": "npm run lintjs -- --watch",
    "prelintsass": "stylefmt --config .stylelintrc --recursive 'src/**/*.scss'",
    "lintsass": "stylelint 'src/**/*.scss'",
    "lintsass:watch": "npm run lintsass -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "ospec",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node build/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/data/db.json --port 3002",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lintjs",
    "build": "webpack --config 'webpack.config.prod.js'",
    "postbuild": "babel-node build/distServer.js",
    "deploy": "surge ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://dontwork@github.com/dontwork/start.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dontwork/start/issues"
  },
  "homepage": "https://github.com/dontwork/start#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.16.3",
    "babili-webpack-plugin": "0.0.7",
    "browser-sync": "^2.18.6",
    "bs-fullscreen-message": "^1.0.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "^2.22.0",
    "install": "^0.8.4",
    "jsdom": "^9.8.0",
    "json-schema-faker": "^0.3.6",
    "json-server": "^0.8.22",
    "localtunnel": "^1.8.1",
    "mocha": "^3.1.2",
    "nock": "^8.1.0",
    "node-sass": "^4.1.1",
    "npm-run-all": "^3.1.1",
    "nsp": "^2.6.2",
    "ospec": "^1.2.3",
    "postcss-loader": "^1.2.1",
    "resource-hints-webpack-plugin": "0.0.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.13.1",
    "stylefmt": "^5.0.3",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-md5-hash": "^0.0.5"
  },
  "dependencies": {
    "mithril": "^1.0.0-rc.8"
  }
}
